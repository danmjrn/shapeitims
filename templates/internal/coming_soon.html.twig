{% extends 'internal_admin.html.twig' %}

{% block title %}{{ 'Alex & Scola\'s Invitation Management System | Dashboard' }}{% endblock %}

{% block body %}
    <div class="content-body">
        <!-- row -->
        <div class="container-fluid">
            <div class="form-head d-flex align-items-center mb-sm-4 mb-3">
                <div class="mr-auto">
                    <h2 class="text-black font-w600">Dashboard</h2>
                    <p class="mb-0">{{ 'Alex & Scola\'s Invitation Management System' }}</p>
                </div>
            </div>
            <div class="row">
                <div class="col-xl-12 col-sm-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="media align-items-center">
                                <div class="media-body mr-3 text-center">
                                    <h2 class="fs-34 text-black font-w600">{{ feature ~ ' Feature Coming Soon' }}</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}

    <!-- Chart piety plugin files -->
    <script src="{{ asset('build/vendor/peity/jquery.peity.min.js') }}"></script>

    <!-- Apex Chart -->
    <script src="{{ asset('build/vendor/apexchart/apexchart.js') }}"></script>

    <!-- Dashboard 1 -->
    <script src="{{ asset('build/js/dashboard/dashboard-1.js') }}"></script>
    <!-- move to includes/scripts 1 -->
    <script>
        $(function () {
            $('#datetimepicker1').datetimepicker({
                inline: true,
            });
        });
    </script>
{% endblock %}
